var update_curr_lbl
var limit_container
var current_text_limit = 100

func _ready():
	limit_container = get_node('Container/HBoxContainer')
	update_curr_lbl = get_node('Container/HBoxContainer/LBLCurr')

func _on_CBTwitter_toggled(button_pressed):
	if button_pressed:
		limit_container.show()
	else:
		limit_container.hide()
